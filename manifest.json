{
    "name": "Quotes Extensions",
    "description" : "Get instipirational quotes",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "Quotes.html"
    },
    "permissions": ["activeTab"],
      "content_scripts":[
        {
          "matches":["<all_urls>"],
          "js":["Quotes.js"]
        }
      ]
  }